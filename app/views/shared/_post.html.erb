<div class="card">
  <%# <%= link_to image_tag(post.image,size: "400x400"),post_path(post.id)%>  
  <% post.images.each do |image| %>
  <%= image_tag image, class: 'images' %>
<% end %>
  <div class="card__body">
    <%= link_to post.title,post_path(post.id), class: :card__title%>
    <%= link_to "by #{post.user.nickname}",root_path, class: :card__user %>
  </div>
</div>